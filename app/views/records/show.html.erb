<p id="notice"><%= notice %></p>

<p>
  <strong>Railcars:</strong>
  <%= @record.railcars_id %>
</p>

<p>
  <strong>Horafinal:</strong>
  <%= @record.horafinal %>
</p>

<p>
  <strong>Estado:</strong>
  <%= @record.estado %><br>
</p>

<% detalle = @record.facturacion %>
<% detalle.each do |elemento|%>
  <strong> <%= elemento[0].round %> minutos </strong>
  <strong> <%= "En el horario de #{elemento[1]} a
  #{elemento[2]}"%> <strong> <br>
  <strong> <%= "Esto te costo #{elemento[3].round}"%> </strong>
  <br>
<% end %>


<%= link_to 'Edit', edit_record_path(@record) %>
<%= link_to 'Back', records_path %>
