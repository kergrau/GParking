<% spaces = Space.new%>
  <% car_ocu = spaces.espacios_ocupados_carros.to_f%>
  <% car_per = (car_ocu / 37) * 100%>
  <div class="medium-3 columns" data-sticky-container>
    <div class="sticky" data-sticky data-anchor="content">
      <h4 align="center">Car ocupation</h4>
      <div class="clipped-circle-graph" data-clipped-circle-graph data-percent=<%= car_per%>>
        <div class="clipped-circle-graph-progress">
          <div class="clipped-circle-graph-progress-fill"></div>
        </div>
        <div class="clipped-circle-graph-percents">
          <div class="clipped-circle-graph-percents-wrapper">
            <span class="clipped-circle-graph-percents-number"></span>
            <span class="clipped-circle-graph-percents-units">of 37</span>
          </div>
        </div>
      </div>

      <br><br>
      <% mot_ocu = spaces.espacios_ocupados_motos.to_f%>
      <% mot_per = (mot_ocu / 12) * 100%>
      <h4 align="center">Moto ocupation</h4>
      <div class="clipped-circle-graph" data-clipped-circle-graph data-percent=<%= mot_per%>>
        <div class="clipped-circle-graph-progress">
          <div class="clipped-circle-graph-progress-fill"></div>
        </div>
        <div class="clipped-circle-graph-percents">
          <div class="clipped-circle-graph-percents-wrapper">
            <span class="clipped-circle-graph-percents-number"></span>
            <span class="clipped-circle-graph-percents-units">of 12</span>
          </div>
        </div>
      </div>
    </div>
  </div>