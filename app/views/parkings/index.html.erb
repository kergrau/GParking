<p id="notice"><%= notice %></p>

<h1>Parkings</h1>

<table>
  <thead>
    <tr>
      <th>Par name</th>
      <th>Par address</th>
      <th>Par state</th>
      <th>Par latitude</th>
      <th>Par longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parkings.each do |parking| %>
      <tr>
        <td><%= parking.par_name %></td>
        <td><%= parking.par_address %></td>
        <td><%= parking.par_state %></td>
        <td><%= parking.par_latitude %></td>
        <td><%= parking.par_longitude %></td>
        <td><%= link_to 'Show', parking %></td>
        <td><%= link_to 'Edit', edit_parking_path(parking) %></td>
        <td><%= link_to 'Destroy', parking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parking', new_parking_path %>
